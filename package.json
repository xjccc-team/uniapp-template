{
  "name": "uni-preset-vue",
  "version": "0.0.0",
  "scripts": {
    "dev:custom": "uni -p",
    "dev:h5": "uni",
    "dev:h5:ssr": "uni --ssr",
    "dev:mp-alipay": "uni -p mp-alipay",
    "dev:mp-baidu": "uni -p mp-baidu",
    "dev:mp-jd": "uni -p mp-jd",
    "dev:mp-kuaishou": "uni -p mp-kuaishou",
    "dev:mp-lark": "uni -p mp-lark",
    "dev:mp-qq": "uni -p mp-qq",
    "dev:mp-toutiao": "uni -p mp-toutiao",
    "dev:mp-weixin": "uni -p mp-weixin",
    "dev:mp-xhs": "uni -p mp-xhs",
    "dev:quickapp-webview": "uni -p quickapp-webview",
    "dev:quickapp-webview-huawei": "uni -p quickapp-webview-huawei",
    "dev:quickapp-webview-union": "uni -p quickapp-webview-union",
    "build:custom": "uni build -p",
    "build:h5": "uni build",
    "build:h5:ssr": "uni build --ssr",
    "build:mp-alipay": "uni build -p mp-alipay",
    "build:mp-baidu": "uni build -p mp-baidu",
    "build:mp-jd": "uni build -p mp-jd",
    "build:mp-kuaishou": "uni build -p mp-kuaishou",
    "build:mp-lark": "uni build -p mp-lark",
    "build:mp-qq": "uni build -p mp-qq",
    "build:mp-toutiao": "uni build -p mp-toutiao",
    "build:mp-weixin": "uni build -p mp-weixin",
    "build:mp-xhs": "uni build -p mp-xhs",
    "build:quickapp-webview": "uni build -p quickapp-webview",
    "build:quickapp-webview-huawei": "uni build -p quickapp-webview-huawei",
    "build:quickapp-webview-union": "uni build -p quickapp-webview-union",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint .",
    "prepare": "husky && pnpm echo:husky",
    "echo:husky": "echo 'npx lint-staged' > .husky/pre-commit && echo 'npx --no-install commitlint --edit \"$1\"' > .husky/commit-msg",
    "commit": "git-cz"
  },
  "dependencies": {
    "@dcloudio/uni-app": "3.0.0-alpha-4060320250423001",
    "@dcloudio/uni-app-harmony": "3.0.0-alpha-4010320240415001",
    "@dcloudio/uni-app-plus": "3.0.0-alpha-4060320250423001",
    "@dcloudio/uni-components": "3.0.0-alpha-3000020210521001",
    "@dcloudio/uni-h5": "3.0.0-alpha-4060320250423001",
    "@dcloudio/uni-mp-alipay": "3.0.0-alpha-4060320250423001",
    "@dcloudio/uni-mp-baidu": "3.0.0-alpha-4060320250423001",
    "@dcloudio/uni-mp-jd": "3.0.0-alpha-4060320250423001",
    "@dcloudio/uni-mp-kuaishou": "3.0.0-alpha-4060320250423001",
    "@dcloudio/uni-mp-lark": "3.0.0-alpha-4060320250423001",
    "@dcloudio/uni-mp-qq": "3.0.0-alpha-4060320250423001",
    "@dcloudio/uni-mp-toutiao": "3.0.0-alpha-4060320250423001",
    "@dcloudio/uni-mp-weixin": "3.0.0-alpha-4060320250423001",
    "@dcloudio/uni-mp-xhs": "3.0.0-alpha-4060320250423001",
    "@dcloudio/uni-quickapp-webview": "3.0.0-alpha-4060320250423001",
    "pinia": "^2.3.1",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.0",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@dcloudio/types": "^3.4.15",
    "@dcloudio/uni-automator": "3.0.0-alpha-4060320250423001",
    "@dcloudio/uni-cli-shared": "3.0.0-alpha-4060320250423001",
    "@dcloudio/uni-stacktracey": "3.0.0-alpha-4060320250423001",
    "@dcloudio/vite-plugin-uni": "3.0.0-alpha-4060320250423001",
    "@types/node": "^22.15.15",
    "@vue/runtime-core": "^3.5.13",
    "@vue/tsconfig": "^0.7.0",
    "commitizen": "^4.3.1",
    "cz-git": "^1.11.1",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.3",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-vue": "^10.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "unocss": "^66.1.0",
    "unocss-preset-weapp": "^66.0.0",
    "unplugin-auto-import": "^19.2.0",
    "unplugin-vue-components": "^28.5.0",
    "vite": "6.3.5",
    "vue-tsc": "^2.2.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,vue}": "eslint . --fix"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
